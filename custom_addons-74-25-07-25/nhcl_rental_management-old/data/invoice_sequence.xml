<?xml version="1.0" encoding="utf-8" ?>
<odoo>
    <data noupdate="1">
        <!-- Rent Invoice Sequence -->
        <record id="seq_invoice_rent" model="ir.sequence">
            <field name="name">Rent Invoice Sequence</field>
            <field name="code">invoice.rent.sequence</field>
            <field name="prefix">RENT/2025/</field>
            <field name="padding">5</field>
            <field name="implementation">standard</field>
            <field name="active" eval="True"/>
        </record>
